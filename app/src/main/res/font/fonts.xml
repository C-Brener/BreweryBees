<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <font
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/quicksand_regular"
        />
    <font
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/quicksand_bold"
        />
    <font
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/roboto_bold"
        />
    <font
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/roboto_regular"
        />
    <font
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/roboto_medium"
        />
    <font
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/roboto_light"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/quicksand_regular"
        tools:targetApi="o"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/quicksand_bold"
        tools:targetApi="o"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/roboto_bold"
        tools:targetApi="o"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/roboto_regular"
        tools:targetApi="o"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/roboto_medium"
        tools:targetApi="o"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/roboto_light"
        tools:targetApi="o"
        />
</font-family>